{
  "scripts": {
    "compile": "npx hardhat clean && npx hardhat compile",
    "clean": "npx hardhat clean",
    "deploy-multilogic": "npx hardhat clean && npx hardhat compile && make restore-artifacts && npx hardhat run ./scripts-hardhat/deploy_multilogic.ts",
    "deploy-lbl": "npx hardhat clean && npx hardhat compile && make restore-artifacts && npx hardhat run ./scripts-hardhat/deploy_lbl.ts",
    "deploy-lbf": "npx hardhat clean && npx hardhat compile && make restore-artifacts && npx hardhat run ./scripts-hardhat/deploy_lbf.ts",
    "deploy-crosschain": "npx hardhat run ./scripts-hardhat/deploy_upgrade.ts --network goerli",
    "deploy-logic": "npx hardhat run ./scripts-hardhat/deploy_storageV2.ts --network bscTestnet",
    "approve-tokens": "npx hardhat run ./scripts-hardhat/approve_swap_deprecated.ts --network goerli",
    "connect-strategies": "npx hardhat clean && npx hardhat compile && npx hardhat run ./scripts-hardhat/connect_strategies.ts",
    "size-contracts": "npx hardhat size-contracts",
    "prettier": "prettier --write contracts/**/*.sol test/**/*.ts",
    "solhint": "solhint contracts/**/*.sol",
    "test": "npx hardhat clean && npx hardhat compile && npx hardhat test",
    "test-local": "npx hardhat clean --config hardhat.config.locsal.ts && npx hardhat --config hardhat.config.local.ts compile && npx hardhat --config hardhat.config.local.ts test --grep hardhat",
    "test:adminable": "npx hardhat test --network hardhat test/suites/adminable.test.ts",
    "test:bolide": "npx hardhat test --network hardhat test/suites/bolide.test.ts",
    "test:treasury": "npx hardhat test --network hardhat test/suites/treasuryVester.test.ts",
    "test:version": "npx hardhat test --network hardhat test/suites/versionable.test.ts",
    "test:storage": "npx hardhat test --network hardhat test/suites/storage.test.ts",
    "test:boosting": "npx hardhat test --network hardhat test/suites/boosting.test.ts",
    "test:privateSale": "npx hardhat test --network hardhat test/suites/privateSale.test.ts",
    "test:logicLiquidity": "npx hardhat test --network bscTestnet test/suites/logic.liquidity.test.ts",
    "test:logicSwap": "npx hardhat test --network bscTestnet test/suites/logic.swap.test.ts",
    "test:reservePair": "npx hardhat test --network bscTestnet test/suites/logic.reservePair.test.ts",
    "test:reserveMasterchef": "npx hardhat test --network bscTestnet test/suites/logic.reserveMasterchef.test.ts",
    "test:multilogicProxy": "npx hardhat test --network bscTestnet test/suites/multilogicproxy.test.ts",
    "test:strategyhelper": "npx hardhat test --network bscTestnet test/suites/strategyhelper.test.ts"
